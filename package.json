    "@angular/router": "^4.0.0-rc.2",
    "core-js": "^2.4.1",
    "es6-promise": "^4.0.5",
    "isomorphic-fetch": "^2.2.1",
    "rxjs": "^5.1.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.6"
  },
